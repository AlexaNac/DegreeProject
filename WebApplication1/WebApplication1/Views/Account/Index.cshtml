@model IEnumerable<WebApplication1.Models.AspNetUser>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>
<p>
    <div class="btn btn-default"> @Html.ActionLink("Add Account", "Register", "Account") </div>
</p>
<hr />
<table class="table table-striped table-hover ">
    <tr class="success">
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr class="active">
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", "Account", new { id = item.Id }, null)
                @Html.ActionLink("Delete", "Delete", "Account", new { id = item.Id }, null)
            </td>
        </tr>
    }

</table>

