@model IEnumerable<WebApplication1.Models.AspNetUser>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>
   <h2>Index</h2>
    <div style="float:right; display:inline-block">
        <div class="glyphicon glyphicon-plus addNew gi-2x" data-type ="Account"></div>
    </div>
</div>
@*<p>
    <div class="btn btn-default"> @Html.ActionLink("Add Account", "Register", "Account") </div>
</p>*@

<hr />
<table class="table table-striped table-hover " data-type="Account">
    <tr class="success">
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr class="active" data-id=@item.Id>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
        </tr>
    }

</table>

